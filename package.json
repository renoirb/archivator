{
  "name": "archivator",
  "version": "1.0.0",
  "description": "Fetch and stream body contents for URLs",
  "repository": {
    "type": "git",
    "url": "git@github.com:renoirb/archivator.git"
  },
  "bugs": {
    "url": "https://github.com/renoirb/archivator/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "babel src/ --minified -d dist/ -s",
    "watch": "babel src/ -d dist/ -w -s",
    "start": "babel-node src/cli.js",
    "lint": "xo"
  },
  "author": "Renoir Boulanger <hello@renoirboulanger.com> (http://renoirb.com/)",
  "license": "MIT",
  "dependencies": {
    "async-file": "^2.0.2",
    "cheerio": "^0.22.0",
    "marked": "*",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "co": "^4.6",
    "eslint": "^3.10.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-unicorn": "^1.0.0",
    "read-all-stream": "~3.1",
    "xo": "^0.17.1"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "plugins": [
      "unicorn"
    ],
    "ignores": [
      "dist/**",
      "example.js"
    ],
    "rules": {
      "func-names": 0
    }
  }
}
