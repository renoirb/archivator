{
  "name": "@archivator/content-divinator",
  "version": "0.0.1",
  "description": "Attempt at guessing stuff, summarize content, based on raw text. A naÃ¯ve Natural Language Processing toolkit.",
  "keywords": [
    "text",
    "processing",
    "stop",
    "words",
    "normalization"
  ],
  "homepage": "https://github.com/renoirb/archivator/blob/v3.x-dev/packages/content-divinator",
  "repository": {
    "type": "git",
    "url": "https://github.com/renoirb/archivator.git",
    "directory": "packages/content-divinator"
  },
  "license": "MIT",
  "author": {
    "name": "Renoir Boulanger",
    "email": "contribs@renoirboulanger.com"
  },
  "exports": {
    "import": "dist/index.mjs",
    "require": "dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "resources"
  ],
  "scripts": {
    "api": "use-run-all api-extractor api-documenter fix",
    "api-documenter": "api-documenter markdown --input temp --output docs",
    "api-extractor": "api-extractor run -c config/api-extractor.json --verbose --local",
    "build": "use-run-all clean build:* api",
    "build:node": "use-bili -c bili.config.ts --format cjs --file-name index.js",
    "build:node-esm": "use-bili -c bili.config.ts --format esm --file-name index.esm.js",
    "clean": "use-rimraf .rpt2_cache dist *.*.log temp",
    "fix": "use-cross-env use-prettier --write  '**/*.{ts,json,md}'",
    "lint": "use-cross-env use-eslint --fix 'src/**/*.ts'",
    "prepublishOnly": "rushx build",
    "test": "use-cross-env use-jest --detectOpenHandles",
    "test:snapshots": "use-cross-env use-jest --detectOpenHandles -u"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.0",
    "@rushstack/node-core-library": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-typescript": "^7.10.0",
    "@microsoft/api-documenter": "^7.8.0",
    "@microsoft/api-extractor": "^7.8.0",
    "@microsoft/node-library-build": "^6.4.0",
    "@renoirb/conventions-use-bili": "^1.1.0-dev",
    "@renoirb/conventions-use-eslint": "^1.1.0-dev",
    "@renoirb/conventions-use-jest": "^1.1.0-dev",
    "@renoirb/conventions-use-prettier": "^1.1.0-dev",
    "@renoirb/conventions-use-typescript-3": "^1.1.0-dev",
    "@renoirb/tools-bundling-helpers": "^1.1.0-dev",
    "@types/jest": "^26.0.0",
    "@types/node": "^10.17.13",
    "core-js": "^3.1.4",
    "ts-jest": "^25.5.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.0"
  },
  "engines": {
    "node": ">=10"
  }
}
